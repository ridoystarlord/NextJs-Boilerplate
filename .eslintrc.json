{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/order": [
      "warn",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "newlines-between": "always"
      }
    ]
  }
}
